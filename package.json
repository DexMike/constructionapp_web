{
  "name": "stargate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --mode=development --colors",
    "build": "webpack --mode production",
    "prestart:dist": "npm run clean-dist && npm run build:html",
    "start:dist": "npx babel-node tools/buildDev.js",
    "poststart:dist": "npx babel-node tools/distServer.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "npx rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "predeployDev": "npm run clean-dist && npm run build:html",
    "deployDev": "npx --max_old_space_size=2048 babel-node tools/buildDev.js",
    "postdeployDev": "aws s3 sync dist/ s3://dev.app.mytrelar.com && aws cloudfront create-invalidation --distribution-id E1LAYT176RPT8J --paths '/*'",
    "predeployDemo": "npm run clean-dist && npm run build:html",
    "deployDemo": "npx --max_old_space_size=2048 babel-node tools/buildDemo.js",
    "postdeployDemo": "aws s3 sync dist/ s3://demo.app.mytrelar.com && aws cloudfront create-invalidation --distribution-id E3VLWJXIOSHO30 --paths '/*'",
    "predeployQa": "npm run clean-dist && npm run build:html",
    "deployQa": "npx --max_old_space_size=2048 babel-node tools/buildQa.js",
    "postdeployQa": "aws s3 sync dist/ s3://qa.app.mytrelar.com && aws cloudfront create-invalidation --distribution-id E2W2Q6ROFYLZDI --paths '/*'",
    "predeployStaging": "npm run clean-dist && npm run build:html",
    "deployStaging": "npx --max_old_space_size=2048 babel-node tools/buildStaging.js",
    "postdeployStaging": "aws s3 sync dist/ s3://app.staging.mytrelar.com && aws cloudfront create-invalidation --distribution-id E3KJAHZFANQPXC --paths '/*'",
    "predeployProd": "npm run clean-dist && npm run build:html",
    "deployProd": "npx --max_old_space_size=2048 babel-node tools/buildProd.js",
    "postdeployProd": "aws s3 sync dist/ s3://app.mytrelar.com && aws cloudfront create-invalidation --distribution-id E1AHYHKY2WVINB --paths '/*'",
    "lint": "npx esw webpack.config.* src"
  },
